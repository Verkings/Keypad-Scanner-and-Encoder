#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
TOOL_HOME=/apps/synopsys/VCSMX/linux;
export TOOL_HOME;
/apps/synopsys/VCSMX/linux/bin/vcs1 -Xrebuild '-Mcfl= -pipe -m32 -O -I/apps/synopsys/VCSMX/include ' '-Mldf=-m32 ' -Mout=simv -Mrun=/apps/synopsys/VCSMX/linux/lib/libvcs.a -Mamsrun= -Mvcsaceobjs= '-Mscsrun= ' -Mcmodrun= '-Mobjects= /apps/synopsys/VCSMX/linux/lib/libvirsim.a /apps/synopsys/VCSMX/linux/lib/librterrorinf.so ' -Mcrtn= -Mcsrc= '-Msyslibs=/apps/synopsys/VCSMX/linux/lib/ctype-stubs_32.a -ldl ' +v2k keypad.v keypad_test.v -gen_obj 2>&1
/apps/synopsys/VCSMX/linux/bin/vcselab -o simv -nobanner 
cd csrc 2>&1
make   -f Makefile  CFLAGS=' -pipe -m32  -O -I/apps/synopsys/VCSMX/include'  ASFLAGS='--32 '   LDFLAGS='-m32 ' DEFAULT_RUNTIME=TRUE product 2>&1
exit $?
